generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model users {
  id         Int    @id @default(autoincrement())
  first_name String @db.VarChar(50)
  last_name  String @db.VarChar(50)
  email      String @unique(map: "constraint_name") @db.VarChar(255)
  password   String
}
